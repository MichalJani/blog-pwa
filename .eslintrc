{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "eslint: recommended",
    "plugin: react/recommended",
    "prettier",
    "prettier/react"
  ],
  "globals": {
    "window": true,
    "document": true,
    "shallow": true,
    "render": true,
    "mount": true,
    "module": true,
    "graphql": true,
    "console": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./src"],
        "extensions": [".js"]
      }
    },
    "import/core-modules": [
      "react",
      "config",
      "react-router",
      "prop-types",
      "styled-components",
      "react-router-dom",
      "react-helmet"
    ]
  },
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "rules":
}
